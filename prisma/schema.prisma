generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model pgmigrations {
  id     Int      @default(autoincrement()) @id
  name   String
  run_on DateTime
}

model users {
  id             Int      @default(autoincrement()) @id
  admin          Int      @default(0)
  createdAt      DateTime @default(now())
  updatedAt      DateTime @default(now())
  passwordDigest String?
  sessionToken   String?
  username       String   @unique
}
